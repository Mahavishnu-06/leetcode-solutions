package Pattern;
public class star14 {
    public static void main(String[] args) {
        star14(6);
    }
    static void star14(int n){
        int k=0;
        for(int i=n;i>=1;i--){
            k++;
           for(int j=1;j<=i;j++){
              System.out.print(k+" ");
           }
           System.out.println();
        }
    }
}
