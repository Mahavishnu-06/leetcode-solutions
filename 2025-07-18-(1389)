class Solution {
    public int[] createTargetArray(int[] nums, int[] index) {
      int ans[]=new int[nums.length];
      ArrayList<Integer> sol=new ArrayList<>();
      for(int i=0;i<nums.length;i++){
        sol.add(index[i],nums[i]);
      }
      for(int i=0;i<nums.length;i++){
        ans[i]=sol.get(i);
      }
       return ans;
    }
}
