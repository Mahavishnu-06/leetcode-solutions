class Solution {
    public String sortSentence(String s) {
        String[] word = s.split(" ");
        String[] ans = new String[word.length];
        for (int i = 0; i < word.length; i++) {
            int n = word[i].length();
            char c = word[i].charAt(n - 1);
            int pos = c - '0' - 1;
            String actualWord = word[i].substring(0, n - 1);
            ans[pos] = actualWord;
        }
        return String.join(" ", ans);
    }
}
