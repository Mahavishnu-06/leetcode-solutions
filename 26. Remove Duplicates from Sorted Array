class Solution {
    public int removeDuplicates(int[] nums) {
        HashSet<Integer> ans= new HashSet<>();
        int n=0;
        for(int i=0;i<nums.length;i++){
           if(!ans.contains(nums[i])){
              ans.add(nums[i]);
              nums[n]=nums[i];
              n++;
           }
        }
        return n;
    }
}
