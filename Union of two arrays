class Solution {
    public int[] unionArray(int[] nums1, int[] nums2) {
        TreeSet<Integer> ans=new TreeSet<>();
        for(int i:nums1){
            ans.add(i);
        }
        for(int i:nums2){
            ans.add(i);
        }
        int arr[]=new int[ans.size()];
        for(int i=0;i<ans.size();i++){
            arr[i]=ans.get(i);
        }
        return arr;
    }
}
